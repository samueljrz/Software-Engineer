{
  "name": "concurso",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "start": "ts-node src/server.ts",
    "dev:debug": "nodemon --inspect src/server.js",
    "queue": "nodemon src/queue.js",
    "typeorm:migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm:migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "typeorm:migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate"
  },
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "allowJs": true
  },
  "javascript.validate.enable": false,
  "dependencies": {
    "@types/sqlite3": "^3.1.7",
    "celebrate": "^13.0.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "nodemon": "^2.0.7",
    "sucrase": "^3.17.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
